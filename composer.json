{
    "name": "phergie/phergie-irc-plugin-react-magic-eight-ball",
    "description": "Phergie plugin for Helping users make decisions",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "license": "BSD-2-Clause",
    "support": {
        "email": "phergie_magic_eightball@davidstockton.com",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/dstockto/phergie-irc-plugin-react-magic-eightball/issues",
        "source": "https://github.com/dstockto/phergie-irc-plugin-react-magic-eightball"
    },
    "minimum-stability": "dev",
    "require": {
        "phergie/phergie-irc-bot-react": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "phake/phake": "1.0.*",
        "satooshi/php-coveralls": "dev-master",
        "codeclimate/php-test-reporter": "0.1.*@dev",
        "satooshi/php-coveralls": "v0.6.1"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\Plugin\\React\\MagicEightBall\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pergie\\Irc\\Tests\\Plugin\\React\\MagicEightBall\\": "tests"
        }
    }
}
